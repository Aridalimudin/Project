<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:background="@drawable/background2"
    tools:context=".ListambulanceActivity">

    <ImageView
        android:id="@+id/back"
        android:layout_width="26dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/red"
        android:src="@drawable/back"
        android:layout_marginLeft="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="-5dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Kontak Darurat Ambulance"
        android:textColor="@color/white"
        android:textSize="23dp"
        android:textStyle="bold"
        android:gravity="center"
        android:paddingTop="18dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="120dp"
        android:orientation="vertical">

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_gravity="center" />

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_marginTop="10dp"
            android:layout_gravity="center" />

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_marginTop="10dp"
            android:layout_gravity="center" />

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_marginTop="10dp"
            android:layout_gravity="center" />

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_marginTop="15dp"
            android:layout_gravity="center" />

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_marginTop="10dp"
            android:layout_gravity="center" />

        <ImageView
            android:layout_width="350dp"
            android:layout_height="70dp"
            android:background="@drawable/img_13"
            android:layout_marginTop="10dp"
            android:layout_gravity="center" />

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="50dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txt1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="8dp"
            android:text="RS Simpangan Depok"
            android:textSize="18dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tx1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt1"
            android:text="1.5 KM"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/txt2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RSU Hasanah Graha Afiah"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_below="@+id/tx1"
            android:paddingTop="38dp"/>

        <TextView
            android:id="@+id/tx2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt2"
            android:text="2.7 KM"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/txt3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RS Citra Medika"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_below="@+id/tx2"
            android:paddingTop="37dp"/>

        <TextView
            android:id="@+id/tx3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt3"
            android:text="4.0 KM"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/txt4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RSUD Hermina Depok"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_below="@+id/txt3"
            android:paddingTop="54dp"/>

        <TextView
            android:id="@+id/tx4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt4"
            android:text="4.9 KM"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/txt5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RS Harapan Depok"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_below="@+id/txt4"
            android:paddingTop="63dp"/>

        <TextView
            android:id="@+id/tx5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt5"
            android:text="6.2 KM"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/txt6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RS Citra Medika"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_below="@+id/txt5"
            android:paddingTop="54dp"/>

        <TextView
            android:id="@+id/tx6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt6"
            android:text="6.8 KM"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/txt7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RSUD Depok"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_below="@+id/txt6"
            android:paddingTop="55dp"/>

        <TextView
            android:id="@+id/tx7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt7"
            android:text="8.4 KM"
            android:paddingLeft="20dp"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="335dp"
        android:orientation="vertical">

        <ImageView
        android:id="@+id/call1"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:background="@drawable/img_15"
        android:layout_marginTop="15dp"/>

        <ImageView
            android:id="@+id/call2"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_below="@+id/call1"
            android:background="@drawable/img_15"
            android:layout_marginTop="55dp"/>

        <ImageView
            android:id="@+id/call3"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_below="@+id/call2"
            android:background="@drawable/img_15"
            android:layout_marginTop="54dp"/>

        <ImageView
            android:id="@+id/call4"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_below="@+id/call3"
            android:background="@drawable/img_15"
            android:layout_marginTop="54dp"/>

        <ImageView
            android:id="@+id/call5"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_below="@+id/call4"
            android:background="@drawable/img_15"
            android:layout_marginTop="55dp"/>

        <ImageView
            android:id="@+id/call6"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_below="@+id/call5"
            android:background="@drawable/img_15"
            android:layout_marginTop="54dp"/>

        <ImageView
            android:id="@+id/call7"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_below="@+id/call6"
            android:background="@drawable/img_15"
            android:layout_marginTop="55dp"/>

    </RelativeLayout>

</RelativeLayout>

